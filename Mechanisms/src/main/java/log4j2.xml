      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.MechanismDesignPrimitives.JointProbabilityMass" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.DomainGenerators.SpatialDomainGenerator" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.CAXOR" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.ProbabilisticCAXOR" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.ECCCoreLLGPayments" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
