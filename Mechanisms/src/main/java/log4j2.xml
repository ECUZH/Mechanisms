    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.ECCVCGPayments" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.VCGPayments" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="ch.uzh.ifi.Mechanisms.CorePayments" level="ERROR" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Root level="debug">
      <AppenderRef ref="Console" level="DEBUG"/>
    </Root>
  </Loggers>
</Configuration>